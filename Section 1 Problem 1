#include <stdio.h>

int multiply(int a, int b) {
    return a * b;
}

int main() {
    int num1, num2, product;

    printf("Enter first number: ");
    scanf("%d", &num1);
    printf("Enter second number: ");
    scanf("%d", &num2);

    product = multiply(num1, num2);

    printf("The product of %d and %d is %d\n", num1, num2, product);

    return 0;
}
